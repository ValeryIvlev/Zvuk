"query getQuickSearch($query: String, $limit: Int, $searchSessionId: String) {\n  quickSearch(query: $query, limit: $limit, searchSessionId: $searchSessionId) {\n    searchSessionId\n    content {\n      __typename\n      ... on Track {\n        id\n        availability\n        title\n        artistNames\n        release {\n          image {\n            src\n          }\n        }\n      }\n      ... on Artist {\n        id\n        title\n        image {\n          src\n        }\n        profile {\n          id\n        }\n      }\n      ... on Release {\n        id\n        availability\n        title\n        artistNames\n        date\n        image {\n          src\n        }\n      }\n      ... on Playlist {\n        id\n        isPublic\n        title\n        image {\n          src\n        }\n        description\n      }\n      ... on Episode {\n        id\n        title\n        availability\n        publicationDate\n        duration\n        podcast {\n          title\n        }\n        image {\n          src\n        }\n      }\n      ... on Podcast {\n        id\n        availability\n        title\n        image {\n          src\n        }\n      }\n      ... on Profile {\n        id\n        name\n        image {\n          src\n        }\n      }\n      ... on Book {\n        id\n        availability\n        title\n        bookAuthors {\n          id\n          rname\n        }\n      }\n      ... on Chapter {\n        id\n        availability\n        title\n        position\n        book {\n          title\n        }\n      }\n    }\n  }\n}"